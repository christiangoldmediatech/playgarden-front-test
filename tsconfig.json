{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "~/*": [
        "./*"
      ]
    },
    "types": [
      "@types/node",
      "@nuxt/types",
      "@nuxtjs/vuetify",
      "@types/jest"
    ]
  },
  "exclude": [
    "node_modules",
    "cypress"
  ],
  "vueCompilerOptions": {
    "experimentalModelPropName": {
      "checked": {
        "input": {
          "type": "checkbox"
        },
        "v-checkbox": true
      }
    }
  }
}
